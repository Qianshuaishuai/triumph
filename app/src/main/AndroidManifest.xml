<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.babyraising.triumph">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.main.MotorSetupActivity"></activity>
        <activity android:name=".ui.profile.ModeActivity" />
        <activity android:name=".ui.other.InfoActivity" />
        <activity android:name=".ui.profile.SystemSettingActivity" />
        <activity android:name=".ui.profile.SettingActivity" />
        <activity android:name=".ui.other.AboutActivity" />
        <activity android:name=".ui.other.HelpActivity" />
        <activity android:name=".ui.other.ContactActivity" />
        <activity android:name=".ui.main.ShutDownActivity" />
        <activity android:name=".ui.main.StopActivity" />
        <activity android:name=".ui.user.UserProfileActivity" />
        <activity android:name=".ui.user.ForgetActivity" />
        <activity android:name=".ui.user.SignInActivity" />
        <activity android:name=".ui.user.RegisterActivity" />
        <activity android:name=".ui.main.WelcomeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>